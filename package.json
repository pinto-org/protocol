  "description": "Pinto algorithmic stablecoin protocol",
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BeanstalkFarms/Beanstalk.git",
    "directory": "protocol"
  },
  "scripts": {
