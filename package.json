    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "chai": "^4.4.1",
    "ethers": "5.7.2",
    "hardhat": "^3.0.6",
    "hardhat-contract-sizer": "^2.8.0",
    "json-bigint": "^1.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-solidity": "^1.4.3",
